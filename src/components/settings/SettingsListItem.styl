.itemContainer {
    flex-direction: row;
    justify-content: space-between;
    padding: 12px 20px;
}

.itemTitle {
    flex-direction: row;
}

.itemIcon {
    height: 25px;
    width: 25px;
    align-self: center;
}

.itemTitleText {
    align-self: center;
    font-size: 15px;
    padding-left: 10px;
    font-weight: 300;
    color: #ffffff;
}

.value {
    padding-right: 16px;
    font-weight: 700;
}

.icon {
    padding-right: 16px;
}

.switch {
    margin-right: 16px;
}

.dropDownSelector {
    flex-direction: row;
    align-items: center;
}

.dropDownSelectorText {
    font-size: 10px;
    color: #ffffff;
    padding-right: 8px;
    text-transform: uppercase;
}

.dropDownSelectorIcon {
    height: 14px;
    width: 14px;
}