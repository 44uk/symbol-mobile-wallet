.defaultTextStyle {
  font-weight: 500;
  padding-top: 0px;
  padding-bottom: 0px;
  padding-left: 0px;
  padding-right: 0px;
}

.activeTextStyle {
  @extend .defaultTextStyle;
  color: #ff00ff;
  font-family: NotoSans-SemiBold;
}

.disabledTextStyle {
  @extend .defaultTextStyle;
  color: #666666;
}

.underlayColor {
  color: #5200c6;
}